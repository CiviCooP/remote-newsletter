<?xml version="1.0"?>
<extension key="remote-newsletter" type="module">
  <file>remote_newsletter</file>
  <name>Remote Newsletter</name>
  <description>Allows subscriptions to CiviCRM entered from a remote drupal 7 site</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Klaas Eikelboom</author>
    <email>klaas.eikelboom@civicoop.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/CiviCooP/remote-newsletter</url>
    <url desc="Documentation">https://github.com/CiviCooP/remote-newsletter/blob/master/README.md</url>
    <url desc="Support">helpdesk@civicoop.org</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-02-03</releaseDate>
  <version>0.8</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>First version, only for testing</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/RemoteNewsletter</namespace>
  </civix>
</extension>
